{
  "name": "atonixcorp-project",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend dashboard for AtonixCorp's automation platform. Built with React, Tailwind, and Redux.",
  "homepage": "https://atonixcorp.com",
  "author": "AtonixCorp",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "tailwind",
    "dashboard",
    "webapp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AtonixCorp/atonixcorp-project.git"
  },
  "bugs": {
    "url": "https://github.com/AtonixCorp/atonixcorp-project/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/AtonixCorp"
  },
  "main": "src/App.js",
  "files": [
    "src",
    "public",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.js",
    "*.jsx",
    "*.json"
  ],
  "engines": {
    "node": ">=24.3.0",
    "npm": ">=11.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:esbuild": "esbuild src/index.jsx --bundle --outfile=public/bundle.js --loader:.jsx=jsx --minify",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,json,css,scss,md}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-after-same-name-blockless",
            "first-nested"
          ],
          "ignore": [
            "after-comment"
          ]
        }
      ],
      "block-no-empty": true,
      "color-no-invalid-hex": true,
      "declaration-colon-space-after": "always",
      "indentation": 2,
      "max-empty-lines": 1,
      "no-duplicate-selectors": true,
      "number-leading-zero": "always",
      "property-no-vendor-prefix": true,
      "selector-pseudo-element-colon-notation": "double",
      "string-quotes": "single",
      "unit-no-unknown": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@redux-devtools/extension": "^3.3.0",
    "auth0-js": "^9.15.0",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "commander": "^14.0.0",
    "css-select": "^6.0.0",
    "eslint": "^9.30.1",
    "helmet": "^8.1.0",
    "hsu-utils": "^0.0.12",
    "install": "^0.13.0",
    "lru-cache": "^11.1.0",
    "mysql2": "^3.14.0",
    "npm": "^11.4.2",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.4.0",
    "react-phone-input-2": "^2.15.1",
    "react-query": "^3.39.2",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.3",
    "react-router-dom": "^7.6.3",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.30.3",
    "react-social-icons": "^6.24.0",
    "react-toastify": "^11.0.5",
    "react-transition-group": "^4.4.5",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^5.1.1",
    "redux-thunk": "^3.1.0",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^11.6.13",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.3",
    "@eslint/js": "^9.0.0",
    "@minify-html/node": "^0.16.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "ajv": "^8.13.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.37.3",
    "html-minimizer-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "install-webpack-plugin": "^4.0.6",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "react-refresh": "^0.17.0",
    "source-map-loader": "^5.0.0",
    "stylelint": "^16.21.1",
    "stylelint-webpack-plugin": "^5.0.1",
    "tailwindcss": "^4.1.11",
    "terser-webpack-plugin": "^5.3.14",
    "vite": "^7.0.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "bin": {
    "string-util": "./cli/string-util.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "resolutions": {
    "core-js": "^3.34.0",
    "eslint": "^8.57.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.5.0"
  }
}
